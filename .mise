    - bash_logout: ~/.bash_logout
    - bash_profile: ~/.bash_profile
    - bashrc: ~/.bashrc
    - cmd-key-happy.lua: ~/.cmd-key-happy.lua
    - cpanreporter/config.ini: ~/.cpanreporter/config.ini
    - cpanreporter/metabase_id.json: ~/.cpanreporter/metabase_id.json
    - dircolors.ansi-dark: ~/.dircolors
    - dzil/config.ini: ~/.dzil/config.ini
