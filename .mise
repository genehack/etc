    - editorconfig: ~/.editorconfig
    - eslintrc: ~/.eslintrc
    - mise: ~/.mise
    - perlcriticrc: ~/.perlcriticrc
    - perltidyrc: ~/.perltidyrc
    - smartcd_config: ~/.smartcd_config
    - tmux.conf: ~/.tmux.conf
    - zshrc: ~/.zshrc
